<main class="main-content content-element">
	<div class="container">
	  {% include 'breadcrumb' %}

		<div class="box-header-intro">
			<label>{{page.title}}</label>	
	    {% if page.content contains '[jvsubheading]' %}
	        {% assign sub = page.content | split:'[/jvsubheading]' | first %}
			{% assign sub = sub | split:'[jvsubheading]' | last %}

	        {% assign des = page.content | split:'[/jvsubheading]' | last %}
	    	{{sub}}
	    {% else %}
			{%assign des = page.content %}
		{% endif %}
		</div>
		{{des}} 

		<h2>jv-sc-team-circle</h2>
		{%include 'jv-sc-team-circle'  with heading_title: 'Demo title', heading_subtitle: 'Demo stitle', heading_enable:true, heading_style: 'hr_center' %}

		<h2>jv-sc-team-gallery</h2>
		{%include 'jv-sc-team-gallery'  with heading_title: 'Demo title', heading_subtitle: 'Demo stitle', heading_enable:true, heading_style: 'hr_center'  %}
		
		<h2>jv-sc-team-rectang</h2>
		{%include 'jv-sc-team-rectang' with heading_title: 'Demo title', heading_subtitle: 'Demo stitle', heading_enable:true, heading_style: 'hr_center'  %}

	</div>
	
</main>