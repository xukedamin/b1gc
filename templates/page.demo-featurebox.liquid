<main class="main-content content-element">
	<div class="container">
	  {% include 'breadcrumb' %}

		<div class="box-header-intro">
			<label>{{page.title}}</label>	
	    {% if page.content contains '[jvsubheading]' %}
	        {% assign sub = page.content | split:'[/jvsubheading]' | first %}
			{% assign sub = sub | split:'[jvsubheading]' | last %}

	        {% assign des = page.content | split:'[/jvsubheading]' | last %}
	    	{{sub}}
	    {% else %}
			{%assign des = page.content %}
		{% endif %}
		</div>
		{{des}} 

		<h2>jv-sc-feature-box</h2>
		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-left',feature_color:'color-dark',feature_bgcolor:'bg-gray',feature_bordercolor:'' %}
		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-left',feature_color:'color-dark',feature_bgcolor:'bg-white',feature_bordercolor:'border-gray' %}
		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-left',feature_color:'color-white',feature_bgcolor:'bg-yellow',feature_bordercolor:'border-yellow' %}
		
		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-left',feature_color:'color-yellow',feature_bgcolor:'bg-white',feature_bordercolor:'border-yellow' %}




		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-right',feature_color:'color-white',feature_bgcolor:'bg-dark',feature_bordercolor:'border-dark' %}
		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-right',feature_color:'color-yellow',feature_bgcolor:'bg-dark',feature_bordercolor:'border-dark' %}

		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-center',feature_color:'color-dark',feature_bgcolor:'bg-white',feature_bordercolor:'border-dark' %}
		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-center',feature_color:'color-dark',feature_bgcolor:'bg-yellow',feature_bordercolor:'border-dark' %}



		{%include 'jv-sc-feature-box' with feature_shape:'i-square ', feature_alignment:'text-center',feature_color:'color-white',feature_bgcolor:'bg-dark',feature_bordercolor:'' %}

		{%include 'jv-sc-feature-box' with feature_shape:'i-circle i-border-double', feature_alignment:'text-center',feature_color:'color-white',feature_bgcolor:'',feature_bordercolor:'border-darkblue' %}




		{%include 'jv-sc-feature-box' with feature_shape:'i-circle', feature_alignment:'text-center box-icon-top border-gray',feature_color:'color-dark',feature_bgcolor:'bg-white',feature_bordercolor:'border-gray',feature_readmore_color:'color-yellow',feature_readmore_pos:'i-before' %}

		{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-center box-icon-top border-gray',feature_color:'color-white',feature_bgcolor:'bg-darkblue',feature_bordercolor:'',feature_readmore_color:'color-yellow',feature_readmore_pos:'i-before' %}


		{%include 'jv-sc-feature-box' with feature_shape:'i-circle', feature_alignment:'text-left box-wrap box-bg-darkblue',feature_color:'color-white',feature_bgcolor:'bg-darkblue',feature_bordercolor:'border-gray',feature_readmore_color:'color-yellow',feature_readmore_pos:'i-before' %}

				{%include 'jv-sc-feature-box' with feature_shape:'i-square', feature_alignment:'text-left box-wrap border-gray',feature_color:'color-white',feature_bgcolor:'bg-darkblue',feature_bordercolor:'',feature_readmore_color:'color-white',feature_readmore_pos:'' %}

		<h2>jv-sc-feature-locator</h2>
		{%include 'jv-sc-feature-locator' %}


	</div>
	
	<div class="container">
		<h2>jv-sc-service-custom</h2>
	</div>
	{%include 'jv-sc-service-custom' %}
</main>