<main class="main-content content-element">
	<div class="container">
	  {% include 'breadcrumb' %}

		<div class="box-header-intro">
			<label>{{page.title}}</label>	
	    {% if page.content contains '[jvsubheading]' %}
	        {% assign sub = page.content | split:'[/jvsubheading]' | first %}
			{% assign sub = sub | split:'[jvsubheading]' | last %}

	        {% assign des = page.content | split:'[/jvsubheading]' | last %}
	    	{{sub}}
	    {% else %}
			{%assign des = page.content %}
		{% endif %}
		</div>
		{{des}} 

		<h2>SC: P1-Tab grid simple</h2>
	  {%include 'jv-sc-product-tabs' %} 
	  
	  <h2>SC: P2- Tab grid Filter 1</h2>
	  {%include 'jv-sc-product-tabs-grid-simple-style-1' %}
	  
	  <h2>SC: P3- Tab grid Filter 2</h2>
	  {%include 'jv-sc-product-tabs-grid-simple-style-2' %}

	  <h2>SC: P4- Tab grid Filter 3</h2>
	  {%include 'jv-sc-product-tabs-grid-simple-style-3' %}
	
 		<h2>SC: P5- Tab grid Leading style 1</h2>
	  {%include 'jv-sc-product-tabs-grid' %} 

	  <h2>SC: P6- Tab grid Leading style 2</h2>
	  {% include 'jv-sc-product-tabs-grid--inline'%}
	  
	  <h2>SC: P7- Slider grid</h2>
	  {%include 'jv-sc-product-slider-grid' %}
		
	</div>

	<div class="container"> <h2>SC: P7- Slider single</h2> </div>
	{%include 'jv-sc-product-slider-single' %} 
</main>