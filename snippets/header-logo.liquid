<a title="{{ shop.name }}" alt="{{ shop.name }}" href="/" rel="home">

{%if settings.logo_type == 'logo_type_image' %}
	<span class="logo-table">
		<span class="logo-table-cell">
			<img src="{{ 'logo-1.png' | asset_url }}" alt="{{ shop.name }}"  class="logo-img-1">
			<img src="{{ 'logo-2.png' | asset_url }}" alt="{{ shop.name }}"  class="logo-img-2" >
		</span>
	</span>

	{%else %}
	<span class="logo-text">
		<h1>{{settings.logo_text_title}}<span class="subtitle">{{settings.logo_text_stitle}}</span></h1>	
	</span>
	{% endif %}

</a>