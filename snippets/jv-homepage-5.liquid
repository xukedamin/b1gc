    {% if settings.home5_pos_content_2_fullwidth == false %}
     </div>
    {% endif %}

  </section>
  {% endif %}


  {% comment %} jv-sc-logopartner  {% endcomment %}
  {% if settings.home5_pos_content_3_enable %}
  <section id="pos--content-3" class="pt-70">
    
    {% if settings.home5_pos_content_3_fullwidth == false %}
     <div class="container">
    {% endif %}

    {% capture file %}{{settings.home5_pos_content_3 }}{% endcapture %}
    {% include file  with heading_enable: settings.home5_pos_content_3_title ,heading_style: settings.home5_pos_content_3_title_style ,heading_title: settings.home5_pos_content_3_title , heading_subtitle: settings.home5_pos_content_3_stitle%}

    {% if settings.home5_pos_content_3_fullwidth == false %}
