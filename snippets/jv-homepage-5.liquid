<main class="page-home-home5 ">
  <header class="home-sticky-nav-trigger">

 {% comment %} ----- Banner  ----- {% endcomment %}
  {%if settings.home5_slide_enable %}
      {% capture slider %}slider-{{settings.home5_slide_style}}{% endcapture %}
      {% include slider  %}
  {%endif %}

  </header>



  {% comment %} jv-sc-banner-3 {% endcomment %}
 {% if settings.home5_pos_content_1_enable %}
  <section id="pos--content-1" class="pt-70">

    {% if settings.home5_pos_content_1_fullwidth == false %}
     <div class="container">
    {% endif %}

    {% capture file %}{{settings.home5_pos_content_1 }}{% endcapture %}
    {% include file with heading_enable: settings.home5_pos_content_1_title ,heading_style: settings.home5_pos_content_1_title_style ,heading_title: settings.home5_pos_content_1_title , heading_subtitle: settings.home5_pos_content_1_stitle %}

    {% if settings.home5_pos_content_1_fullwidth == false %}
     </div>
    {% endif %}

  </section>
  {% endif %}

  
  {% comment %}  'jv-sc-product-tabs-grid-simple' with tab_style:'2',product_paginav:false, product_loadmore:true, product_desc_enable: true  {% endcomment %}
  {% if settings.home5_pos_content_2_enable %}
  <section id="pos--content-2" class="pt-70">

    {% if settings.home5_pos_content_2_fullwidth == false %}
